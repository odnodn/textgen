<div class="card">
    <div class="card-header">
        <h4 class="mb-0">  {{ '{{' }} entity.id  {{ '}}' }} -  {{ '{{' }}
            entity.dateCreated | date {{ '}}' }} </h4>
    </div>
    <div class="card-body">
        {% for prop in genUtils.allProps(entity) -%}
            {{ '{{' }} entity.{{prop.name}} {{ '}}' }}
        {% endfor %}
    </div>
    <div class="card-footer">
        <button class="btn btn-default" (click)="edit(e)">
            <span class="fa fa-edit"></span>
        </button>
        <button class="btn btn-default" (click)="delete(e)">
            <span class="fa fa-trash"></span>
        </button>
    </div>
</div>