package helpchild {

enum Gender {
    MALE
    FEMALE
}

abs entity Person {
    firstName: string
    lastName: string
    gender:Gender
    dob:date
}

entity Child extends Person {

    description: text
    interests: string
    parent: Parent opt

}

entity Parent extends Person{

    //children : Child  many
    email : string
    city: string
    country: string
    bankInfo: string
}

entity Quote {
    tags: string
    text: text
    explanation: text
    page: int opt max:1430
}




}//end of package comm

