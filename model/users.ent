package pets {

    enum Status {
        RCVD
        ORDRD
        DLVRD
    }

    entity  Pet{
        name: string pattern:'^[\w\s-]+$'
        price: currency
        type : PetType opt
        parentType : PetType opt
        dob: date
        weight: int max:10
        neutered: bool
        description: text
        status: Status
        skills : Skill many embedded
    }

    enum CoverageLevel {
        ONE
        TWO
        TWOTHOSUAND_PLUS

    }

    enum PlanType  {
        SINGLE
        MULTI
    }

    enum WhosTravelling  {
        SINGLE
        FAMILY
    }

entity QuoteData{
    haveCare: bool
    alreadyDeparted: bool
    over60: bool 
    departure: date
    returnDate: date

    dobOldest: date
    planMember: bool 
    groupNumber: int
    idNumber: string
    planType : PlanType
    interestedExtra: bool 
    initialPaymentWithinLast72: bool 
    coverageLevel: CoverageLevel
    chosenExtra: bool 
    travellingForMedical: bool 
    travellingAgainstAdvice: bool 
    twoOrMoreStrokes: bool 
    icd : bool 
    homeOxygen : bool 
    terminal: bool 
    ninetyDayStable: bool 
    planTotal: currency
    trn: int
}

entity Skill {
    name: string
    level : int
}

entity PetType {
    name : string
}

}