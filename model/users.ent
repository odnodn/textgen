package pets {

        enum Status {
            RCVD
            ORDRD
            DLVRD
        }

entity  Pet{
    name: string
    price: currency
    type : PetType
    parentType : PetType
    dob: date
    weight: int
    neutered: bool
    description: text
    status: Status
    skills : Skill many embedded
}

entity Skill {
    name: string
    level : int
}

entity PetType {
    name : string
}

}